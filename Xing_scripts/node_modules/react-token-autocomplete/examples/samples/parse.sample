<TokenAutocomplete
  parseToken={
    function(value){
      return value.name;
    }
  }
  parseOption={
    function(value){
      return value.name;
    }
  }
  parseCustom={
    function(value){
      return {
        name: value
      }
    }
  }
  options={[
    {name: 'apple'},
    {name: 'banana'},
    {name: 'carrot'},
    {name: 'watermelon'}
  ]}
/>
